Cell[TextData[{
 "The velocity Jacobian is ",
 Cell[BoxData[
  FormBox[
   RowBox[{"J", "=", 
    RowBox[{"(", "\[NoBreak]", GridBox[{
       {
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{
            RowBox[{"-", 
             SubscriptBox["r", "2"]}], " ", 
            SubscriptBox["c", "2"]}], "-", 
           RowBox[{
            SubscriptBox["r", "3"], " ", 
            SubscriptBox["c", "23"]}]}], ")"}], " ", 
         SubscriptBox["s", "1"]}], 
        RowBox[{
         RowBox[{
          RowBox[{"-", 
           SubscriptBox["r", "2"]}], " ", 
          SubscriptBox["c", "1"], " ", 
          SubscriptBox["s", "2"]}], "-", 
         RowBox[{
          SubscriptBox["r", "3"], " ", 
          SubscriptBox["c", "1"], " ", 
          SubscriptBox["s", "23"]}]}], 
        RowBox[{
         RowBox[{"-", 
          SubscriptBox["r", "3"]}], " ", 
         SubscriptBox["c", "1"], " ", 
         SubscriptBox["s", "23"]}]},
       {
        RowBox[{
         SubscriptBox["c", "1"], " ", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{
            SubscriptBox["r", "2"], " ", 
            SubscriptBox["c", "2"]}], "+", 
           RowBox[{
            SubscriptBox["r", "3"], " ", 
            SubscriptBox["c", "23"]}]}], ")"}]}], 
        RowBox[{
         RowBox[{
          RowBox[{"-", 
           SubscriptBox["r", "2"]}], " ", 
          SubscriptBox["s", "1"], " ", 
          SubscriptBox["s", "2"]}], "-", 
         RowBox[{
          SubscriptBox["r", "3"], " ", 
          SubscriptBox["s", "1"], " ", 
          SubscriptBox["s", "23"]}]}], 
        RowBox[{
         RowBox[{"-", 
          SubscriptBox["r", "3"]}], " ", 
         SubscriptBox["s", "1"], " ", 
         SubscriptBox["s", "23"]}]},
       {"0", 
        RowBox[{
         RowBox[{
          SubscriptBox["r", "2"], " ", 
          SubscriptBox["c", "2"]}], "+", 
         RowBox[{
          SubscriptBox["r", "3"], " ", 
          SubscriptBox["c", "23"]}]}], 
        RowBox[{
         SubscriptBox["r", "3"], " ", 
         SubscriptBox["c", "23"]}]}
      },
